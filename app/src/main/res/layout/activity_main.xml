<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">

        <Button
            android:id="@+id/btn_create"
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:text="Create操作符" />

        <Button
            android:id="@+id/btn_just"
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:text="just操作符" />

        <Button
            android:id="@+id/btn_map"
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:text="map操作符" />

        <Button
            android:id="@+id/btn_from"
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:text="from类操作符" />

        <Button
            android:id="@+id/btn_interval"
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:text="interval操作符" />

        <Button
            android:id="@+id/btn_interval_range"
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:text="intervalRange操作符" />


        <Button
            android:id="@+id/btn_flatmap"
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:text="flatmap操作符" />


    </LinearLayout>

</ScrollView>